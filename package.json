{
  "name": "type-done",
  "version": "2.0.3",
  "description": "Install missing DefinitelyTyped packages in one click",
  "license": "MIT",
  "repository": "seognil-lab/type-done",
  "type": "module",
  "exports": "./dist/app.js",
  "bin": {
    "type-done": "dist/app.js"
  },
  "author": {
    "name": "Seognil LC",
    "email": "seognil@gmail.com",
    "url": "https://github.com/seognil"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "build": "tsup src/app.ts --format=esm",
    "release": "release-it --'hooks.after:bump=\"npm run build\"'",
    "pretty": "prettier -w {src,test}/** --plugin-search-dir=."
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "index.js",
    "readme*"
  ],
  "keywords": [
    "ts",
    "typescript",
    "type",
    "types",
    "typing",
    "typings",
    "definitelytyped",
    "installer",
    "tool"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "command-exists": "^1.2.8",
    "figures": "^4.0.0",
    "jsonfile": "^6.1.0",
    "node-fetch": "^3.0.0",
    "ora": "^6.0.1",
    "p-limit": "^4.0.0",
    "pkg-up": "^3.1.0",
    "pretty-ms": "^7.0.0",
    "registry-url": "^6.0.0",
    "sort-keys": "^5.0.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/jsonfile": "^6.0.1",
    "@types/node": "^16.9.1",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.2",
    "@types/yargs": "^17.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.3",
    "release-it": "^14.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "typescript": "^4.4.3",
    "vitest": "^0.12.6"
  }
}
