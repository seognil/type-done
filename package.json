{
  "name": "type-done",
  "version": "2.0.3",
  "description": "Install missing DefinitelyTyped packages in one click",
  "license": "MIT",
  "repository": "seognil-lab/type-done",
  "type": "module",
  "exports": "./dist/app.js",
  "bin": {
    "type-done": "dist/app.js"
  },
  "author": {
    "name": "Seognil LC",
    "email": "seognil@gmail.com",
    "url": "https://github.com/seognil"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "build": "tsup src/app.ts --format=esm",
    "build:watch": "tsup src/app.ts --format=esm --watch",
    "release": "release-it --'hooks.after:bump=\"npm run build\"'",
    "pretty": "prettier -w {src,test}/** --plugin-search-dir=."
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/app.js",
    "readme*"
  ],
  "keywords": [
    "ts",
    "typescript",
    "type",
    "types",
    "typing",
    "typings",
    "definitelytyped",
    "installer",
    "tool"
  ],
  "dependencies": {
    "chalk": "^5.0.1",
    "command-exists": "^1.2.9",
    "figures": "^4.0.1",
    "jsonfile": "^6.1.0",
    "node-fetch": "^3.2.4",
    "ora": "^6.1.0",
    "p-limit": "^4.0.0",
    "pkg-up": "^4.0.0",
    "pretty-ms": "^7.0.1",
    "registry-url": "^6.0.0",
    "sort-keys": "^5.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/jsonfile": "^6.0.1",
    "@types/node": "^17.0.34",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2.6.1",
    "@types/rimraf": "^3.0.2",
    "@types/yargs": "^17.0.10",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vitest": "^0.12.6"
  }
}
